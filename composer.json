{
    "name": "treblle/treblle-laravel",
    "description": "Stay in tune with your APIs",
    "license": "MIT",
    "homepage": "https://treblle.com/",
    "type": "library",
    "keywords": [
        "api",
        "debuging",
        "documentation",
        "laravel",
        "monitoring",
        "treblle"
    ],
    "authors": [
        {
            "role": "Developer",
            "name": "Vedran CindriÄ‡",
            "email": "vedran@treblle.com",
            "homepage": "https://treblle.com/"
        },
        {
            "role": "Developer",
            "name": "Steve McDougall",
            "email": "juststevemcd@gmail.com",
            "homepage": "https://treblle.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Treblle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Treblle\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/rwatson462/php-utils.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/rwatson462/sdk-tools.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/rwatson462/treblle-sdk-core.git"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "treblle/treblle-sdk-core": "^0.0.4",
        "treblle/utils": "^0.0.35"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.5.1",
        "laravel/octane": "*",
        "laravel/pint": "^1.10",
        "orchestra/testbench": "^7.0 || ^8.5.1",
        "phpstan/phpstan": "^1.10.14"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Treblle\\TreblleServiceProvider"
            ]
        }
    },
    "scripts": {
        "pint": [
            "./vendor/bin/pint"
        ],
        "stan": [
            "./vendor/bin/phpstan analyse"
        ],
        "test": [
            "./vendor/bin/phpunit"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        }
    }
}
